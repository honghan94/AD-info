@model MyProject.Controllers.CreateModel

@Html.AntiForgeryToken()
@if (ViewBag.Message != null)
{
    <span class="text-success">@ViewBag.Message</span>
}
<span class="alert-danger">
    @Html.ValidationSummary()
</span>
<div id="load-ajax" style="display: none;">
    <div class="spinner">Loading...</div>
</div>
<div class="form-group">
    Username :
    @Html.TextBoxFor(model => model.SamAccountName, new { @class = "form-control" })
</div>
<div class="form-group">
    Display Name :
    @Html.TextBoxFor(model => model.DisplayName, new { @class = "form-control" })
</div>
<div class="form-group">
    New Password :
    @Html.PasswordFor(model => model.Password, new { @class = "form-control" })
</div>
<div class="form-group">
    Confirmation Password :
    @Html.PasswordFor(model => model.SecPass, new { @class = "form-control" })
</div>
<div class="form-group">
    Org Unit :
    @Html.DropDownListFor(model => model.SelectedOU, Model.OrgUnit, new { @class = "form-control" })
</div>